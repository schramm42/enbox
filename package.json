{
  "name": "enbox",
  "version": "0.0.0",
  "description": "oclif example Hello World CLI",
  "author": "Thomas Schramm",
  "bin": {
    "enbox": "./bin/run"
  },
  "homepage": "https://github.com/schramm42/enbox",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "schramm42/enbox",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.0",
    "colors": "^1.4.0",
    "inquirer": "^8.2.0",
    "openpgp": "^5.1.0",
    "reflect-metadata": "^0.1.13",
    "validator": "^13.7.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/colors": "^1.2.1",
    "@types/inquirer": "^8.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/openpgp": "^4.4.18",
    "@types/reflect-metadata": "^0.1.0",
    "@types/validator": "^13.7.1",
    "@types/winston": "^2.4.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "enbox",
    "dirname": "enbox",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "watch": "tsc -w -p tsconfig.json",
    "init": "node bin/dev init ./.vscode/tmp -e test@gmail.com",
    "reset": "rm -rf ./.vscode/tmp",
    "ls": "node bin/dev ls ./.vscode/tmp"
  },
  "engines": {
    "node": ">=17.3.0"
  },
  "bugs": "https://github.com/schramm42/enbox/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
