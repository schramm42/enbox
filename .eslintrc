{
  "extends": [
    "plugin:node/recommended",
    "eslint:recommended",
    "oclif",
    "oclif-typescript"
  ],
  "rules": {
    "object-curly-spacing": "off",
    "node/no-missing-import": "off",
    "@typescript-eslint/no-inferrable-types": "off",
    "no-negated-condition": "off",
    "comma-dangle": "off"
  }
}
